{
  "schemaVersion": 1,
  "id": "vividological",
  "version": "${version}",
  "name": "Vividological",
  "description": "Cool materials! Deadly weapons! Funky trees! Stupid food! And more!!",
  "authors": ["Erisatis Alterna (code), Dread (textures), Hayes (ideas), Sue (ideas), KindlingCinder (textures we recolored for compat reasons)"],
  "contact": {},
  "license": "WTFPL",
  "icon": "assets/vividological/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "gay.invis.vividological.client.VividologicalDataGenerator"
    ],
    "client": [
      "gay.invis.vividological.client.VividologicalClient"
    ],
    "main": [
      "gay.invis.vividological.Vividological"
    ]
  },
  "mixins": [
    "vividological.mixins.json",
    {
      "config": "vividological.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "trinkets": "3.10.0",
    "fractal": "1.3.0+1.21"
  }
}
